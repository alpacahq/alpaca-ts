{
  "name": "@master-chief/alpaca",
  "version": "4.0.3",
  "description": "A TypeScript Node.js library for the https://alpaca.markets REST API and WebSocket streams.",
  "author": "117",
  "contributors": [
    "lbstr",
    "AqilCont",
    "KalebMills",
    "anonrose",
    "husayt"
  ],
  "license": "ISC",
  "keywords": [
    "alpaca",
    "alpaca-trade-api-js",
    "alpaca trade api",
    "trading",
    "exchange",
    "bot",
    "alpaca markets",
    "markets",
    "stocks",
    "stock"
  ],
  "type": "module",
  "types": "./@types/index.d.ts",
  "typings": "./@types/index.d.ts",
  "unpkg": "./dist/alpaca.js",
  "jsdelivr": "./dist/alpaca.js",
  "browser": "./dist/alpaca.browser.min.js",
  "main": "./dist/mjs/index.js",
  "module": "./dist/mjs/index.js",
  "exports": {
    "node": "./dist/mjs/index.js",
    "import": "./dist/mjs/index.js",
    "default": "./dist/alpaca.browser.min.js"
  },
  "engines": {
    "node": ">=14"
  },
  "files": [
    "src/",
    "dist/",
    "@types/"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/117/alpaca"
  },
  "scripts": {
    "build": "npm run clean && npm i && npm run build:mjs && npm run build:esm  && npm run build:browser && npm run build:es-bundle",
    "build:mjs": "tsc --outDir dist/mjs  --allowSyntheticDefaultImports -d --declarationDir @types --module esnext --target esnext",
    "build:esm": "rollup -c config/rollup/dist/es.js",
    "build:es-bundle": "rollup -c config/rollup/dist/es-bundle.js",
    "build:browser": "rollup -c config/rollup/dist/browser.js",
    "clean": "rimraf dist @types",
    "test": "npm run test:default",
    "test:default": "jest",
    "test:watch": "jest --watchAll"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/test"
    ],
    "testMatch": [
      "**/*.test.ts"
    ]
  },
  "dependencies": {
    "abort-controller": "^3.0.0",
    "bottleneck": "^2.19.5",
    "bufferutil": "^4.0.3",
    "eventemitter3": "^4.0.7",
    "isomorphic-unfetch": "^3.1.0",
    "isomorphic-ws": "^4.0.1",
    "qs": "^6.9.6",
    "utf-8-validate": "^5.0.4",
    "ws": "^7.4.3"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.1.1",
    "@rollup/plugin-typescript": "^8.1.1",
    "@types/jest": "^26.0.20",
    "@types/node-fetch": "^2.5.8",
    "@types/qs": "^6.9.5",
    "@types/ws": "^7.4.0",
    "jest": "^26.6.3",
    "rimraf": "^3.0.2",
    "rollup": "^2.38.4",
    "rollup-plugin-terser": "^7.0.2",
    "ts-jest": "^26.5.0",
    "tslib": "^2.0.3",
    "typescript": "^4.2.0-beta"
  }
}
